# Current GAMS Studio version.
STUDIO_MAJOR_VERSION=1
STUDIO_MINOR_VERSION=20
STUDIO_PATCH_LEVEL=2
VERSION='$$STUDIO_MAJOR_VERSION'.'$$STUDIO_MINOR_VERSION'.'$$STUDIO_PATCH_LEVEL'

# Minimum required GAMS Distribution version, which
# is the latest official GAMS release.
GAMS_DISTRIB_MAJOR=49
GAMS_DISTRIB_MINOR=1
GAMS_DISTRIB_GOLD=0
GAMS_DISTRIB_VERSION=$$GAMS_DISTRIB_MAJOR$$GAMS_DISTRIB_MINOR$$GAMS_DISTRIB_GOLD

# Project file version defines
DEFINES += 'STUDIO_VERSION_STRING=\\"$$VERSION\\"'
DEFINES += 'STUDIO_MAJOR_VERSION=$$STUDIO_MAJOR_VERSION'
DEFINES += 'STUDIO_MINOR_VERSION=$$STUDIO_MINOR_VERSION'
DEFINES += 'STUDIO_PATCH_LEVEL=$$STUDIO_PATCH_LEVEL'
DEFINES += 'GAMS_DISTRIB_MAJOR=$$GAMS_DISTRIB_MAJOR'
DEFINES += 'GAMS_DISTRIB_MINOR=$$GAMS_DISTRIB_MINOR'
DEFINES += 'GAMS_DISTRIB_VERSION=$$GAMS_DISTRIB_VERSION'
DEFINES += 'GAMS_DISTRIB_VERSION_SHORT=\\"'$$GAMS_DISTRIB_MAJOR'.'$$GAMS_DISTRIB_MINOR'\\"'
DEFINES += 'GAMS_VERSION_STRING=\\"'$$GAMS_DISTRIB_MAJOR'.'$$GAMS_DISTRIB_MINOR'.'$$GAMS_DISTRIB_GOLD'\\"'
