pipeline {
     agent any 
     stages {
         stage('Build') {
             steps {
                 parallel linux: {
                     node('master') {
                         checkout scm
                         sh 'qmake src/ide.pro && make -j3'
                     }
                 },
                 windows: {
                     node('windows') {
                         checkout scm
                         //bat 'qmake src/ide.pro && make -j3'
                     }
                 },
                 macos: {
                     node('macos') {
                         checkout scm
                         sh 'qmake src/ide.pro && make -j3'
                     }
                 }
             }
         }
     }
}
